{
  "name": "zedo",
  "version": "0.0.4",
  "description": "zedo is a language-agnostic and deterministic package manager",
  "type": "module",
  "bin": {
    "zedo": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown src/cli.ts --format esm --dts false --clean",
    "dev": "tsx src/cli.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "generate:schema": "tsx scripts/generate-schema.ts",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "execa": "^9.6.1",
    "fs-extra": "^11.3.3",
    "os": "^0.1.2",
    "path": "^0.12.7",
    "semver": "^7.7.4",
    "yaml": "^2.4.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/semver": "^7.7.1",
    "tsdown": "^0.20.3",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0"
  }
}
